		"RTP_OPEN_ACK"                  { GetInstRtp }
		"RTP_OPEN_NAK"                  { GetInstRtp }
		"RAB_OPEN_ACK"                  { GetInstCsUdg }
		"RAB_OPEN_NAK"                  { GetInstCsUdg }
