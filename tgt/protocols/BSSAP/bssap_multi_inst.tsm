		"SCCP_N_NOTICE_IND"				{ DecSccpNNoticeInd }
		"BSSAP_AP_EST_ACK"  			{ ApEstAckHdl }
		"BSSAP_AP_EST_NACK"  	        { ApEstNackHdl }
		"T_PAG"           				{ DefaultTmrMs }
		"T_CIPHR"    					{ DefaultTmrMs }
		"T_RG_TRR2"						{ DefaultTmrMs }
		"T_DBG_GET_STATS"      			{ GetStatistics }
